{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "ZeroImgur",
  "short_name": "ZeroImgur",
  "description": "This extension makes your browser load Imgur content from 0imgur.com instead of imgur.com.",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/19.png"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions":["*://*.imgur.com/*","webRequest","webRequestBlocking"]
}
